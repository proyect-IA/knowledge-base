[
class(nombreClase,nombrePadre,[ [propiedad => (valor,peso), propiedad] ,[ (propiedad=>valor)=>>(propiedad,peso)],relaciones,individuos),

class(top,none,[],[],[]).

//Aqui esta un ejemplo de como seran con las preferencias
class(comida,top,[ [sabor, salada => (si,0)],[(sabor, salada=>si) =>> (deliciosa,2) )]],[],[]),
]
